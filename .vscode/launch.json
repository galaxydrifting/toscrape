{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Scrapy: crawl bookspider",
            "type": "python",
            "request": "launch",
            "module": "scrapy",
            "args": [
                "crawl",
                "bookspider",
            ],
            "cwd": "${workspaceFolder}/bookscraper/bookscraper",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        }
    ]
}